{
  "name": "@dendronhq/plugin-core",
  "displayName": "dendron",
  "description": "schema based note taking",
  "publisher": "dendron",
  "version": "0.1.6",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/dendronhq/dendron.git",
    "directory": "packages/plugin-core"
  },
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dendron.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "dendron.lookup",
        "title": "Lookup"
      },
      {
        "command": "dendron.workspaceInit",
        "title": "Setup"
      }
    ],
    "keybindings": [
      {
        "command": "dendron.lookup",
        "mac": "cmd+p"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "lint": "eslint src --ext ts",
    "watch": "npm run compile -- --watch",
    "build": "npm run compile",
    "compile": "tsc -p tsconfig.build.json",
    "pretest": "npm run build && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.155",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@dendronhq/common-all": "^0.1.6",
    "@dendronhq/common-server": "^0.1.6",
    "@dendronhq/engine-server": "^0.1.6",
    "lodash": "^4.17.15"
  },
  "gitHead": "403eb012f8b34bc115dd7dd86f80c76b39329f3b"
}

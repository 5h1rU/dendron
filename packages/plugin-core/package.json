{
  "name": "@dendronhq/plugin-core",
  "displayName": "dendron",
  "description": "Dendron is a hierarchal note taking tool that grows as you do. ",
  "publisher": "dendron",
  "version": "0.3.1",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/dendronhq/dendron.git",
    "directory": "packages/plugin-core"
  },
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "second brain",
    "markdown",
    "roam",
    "notes",
    "schema"
  ],
  "preview": true,
  "extensionDependencies": [],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Root Dir",
      "properties": {
        "dendron.rootDir": {
          "type": "string",
          "default": false,
          "description": "Where dendron will store notes"
        }
      }
    },
    "commands": [
      {
        "command": "dendron.lookup",
        "title": "Dendron: Lookup"
      },
      {
        "command": "dendron.initWS",
        "title": "Dendron: Initialize Workspace"
      },
      {
        "command": "dendron.changeWS",
        "title": "Dendron: Change Workspace"
      },
      {
        "command": "dendron.deleteNode",
        "title": "Dendron: Delete Node"
      },
      {
        "command": "dendron.reloadWS",
        "title": "Dendron: Reload Workspace"
      },
      {
        "command": "dendron.dev.resetConfig",
        "title": "Dev:Dendron: ResetConfig"
      }
    ],
    "keybindings": [
      {
        "command": "dendron.lookup",
        "mac": "ctrl+p"
      }
    ]
  },
  "icon": "assets/images/icon.png",
  "scripts": {
    "vscode:prepublish": "npm run build || true",
    "lint": "eslint src --ext ts",
    "watch": "npm run compile -- --watch",
    "build": "npm run compile",
    "compile": "tsc -p tsconfig.build.json",
    "pretest": "",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/execa": "^2.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.155",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@dendronhq/common-all": "^0.3.0",
    "@dendronhq/common-server": "^0.3.0",
    "@dendronhq/engine-server": "^0.3.0",
    "execa": "^4.0.2",
    "lodash": "^4.17.15"
  },
  "gitHead": "74f4be7ed8b97d566cd905a1409a347668a3aa8a"
}

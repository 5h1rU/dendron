{
  "name": "@dendronhq/express-server",
  "version": "0.12.3-alpha.15",
  "scripts": {
    "build": "node build.js && tsc --build tsconfig.prod.json",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "watch": "tsc --watch",
    "test": "NODE_OPTIONS='--unhandled-rejections=strict' npx jest",
    "start:integ": "env PORT=3005 nodemon"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node ./src/start.ts"
  },
  "dependencies": {
    "@dendronhq/engine-server": "^0.12.3-alpha.14",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.1.1",
    "http-status-codes": "^2.1.4",
    "jsonfile": "^6.0.1",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "tslib": "^2.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.48",
    "@types/jest": "^26.0.14",
    "@types/jsonfile": "^6.0.0",
    "@types/lodash": "^4.14.161",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.2",
    "@types/supertest": "^2.0.10",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jest": "^23.6.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "gitHead": "3c8fb7d88198badb6f46705eb3d26adaf50ff916"
}

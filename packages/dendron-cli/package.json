{
  "name": "@dendronhq/dendron-cli",
  "version": "0.5.2",
  "description": "dendron-cli",
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/dendronhq/dendron.git",
    "directory": "packages/common-server"
  },
  "author": {
    "name": "Kevin Lin",
    "email": "kevin@thence.io",
    "url": "https://kevinslin.com",
    "organization": true
  },
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/src/index",
  "types": "lib/src/index",
  "scripts": {
    "clean": "rimraf lib && rimraf coverage",
    "format": "echo foo",
    "lint": "echo stub",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && npm run format && npm run lint && echo Using TypeScript && tsc --version",
    "build": "npm run compile",
    "compile": "tsc -p tsconfig.build.json",
    "test": "env LOG_DST=/tmp/dendron-engine-server.log jest",
    "test:unit:all": "cross-env LOG_LEVEL=error yarn test",
    "coverage": "jest --coverage",
    "watch": "npm run compile -- --watch",
    "test:watch": "npm run test -- --watch",
    "test:unit:debug": "NODE_ENV=test  node --inspect node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@dendronhq/common-all": "^0.5.2",
    "@dendronhq/common-server": "^0.5.2",
    "@dendronhq/engine-server": "^0.5.2",
    "fs-extra": "^9.0.1",
    "gray-matter": "^4.0.2",
    "lodash": "^4.17.19",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^23.3.3",
    "@types/lodash": "^4.14.158",
    "@types/node": "13.11.0",
    "@types/yargs": "^15.0.5",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
